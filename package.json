{
	"name": "laravue-mailer",
	"version": "1.0.4",
	"private": false,
	"author": "Damilola Olowookere <damms005@gmail.com>",
	"homepage": "https://github.com/damms005/laravue-mailer-js#readme",
	"description": "Vue Web Component for formatting email message based on CSV file content. Sample backend (Laravel package): https://github.com/damms005/laravue-mailer",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/damms005/laravue-mailer-js.git"
	},
	"keywords": [
		"Email sender",
		"CSV Editor",
		"TSV Editor",
		"Vue Js",
		"Laravel",
		"TailwindCss"
	],
	"bugs": {
		"url": "https://github.com/damms005/laravue-mailer-js/issues"
	},
	"scripts": {
		"development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
		"watch": "npm run development -- --watch",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"wc": "vue-cli-service build --target wc --name laravue-mailer src/App.vue"
	},
	"dependencies": {
		"core-js": "^2.6.11",
		"fakergem": "^1.0.3",
		"loadash": "^1.0.0",
		"tailwindcss": "^1.8.4",
		"tinymce": "^5.4.2",
		"vue": "^2.6.12",
		"vue-mce": "^1.5.3"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.12.1",
		"@vue/cli-plugin-eslint": "^3.12.1",
		"@vue/cli-service": "^3.12.1",
		"babel-eslint": "^10.1.0",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"vue-template-compiler": "^2.6.12",
		"webpack-cli": "^3.3.12"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
